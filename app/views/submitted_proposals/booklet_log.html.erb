<main class="content">
  <div class="container-fluid">
    <div class="header">
      <h1 class="header-title">LaTex log</h1>
    </div>
    <div class="card">
      <div class="card-body">
        <div class="row">
          <%= simple_format(@log.log) %>
        </div>
        <div class="row">
        <%= link_to "Source file", download_file_submitted_proposals_path(log_id: @log) %>
        </div>
      </div>
    </div>
  </div>
</main>

